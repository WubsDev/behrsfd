    <validationRules>
        <fullName>BEHR_Lost_Oppty_Closed_Competitors_US</fullName>
        <active>true</active>
        <description>All lost opportunities must have a closed competitors.</description>
        <errorConditionFormula>AND( ISPICKVAL(  StageName  , &quot;Closed Lost&quot;) ,   ISNULL(Competitors__c) )</errorConditionFormula>
        <errorDisplayField>Competitors__c</errorDisplayField>
        <errorMessage>All opportunities that have been lost must have a competitors.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contact_Check_rule</fullName>
        <active>true</active>
        <description>An Account should have tagged to a Contact</description>
        <errorConditionFormula>AND(ISBLANK(Primary_Contact__c),Count_Opportunity_Product__c&gt;0)</errorConditionFormula>
        <errorMessage>Contact is required When Opportunity Product exist</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lost_Oppty_Closed_Reason</fullName>
        <active>true</active>
        <description>All lost opportunities must have a closed reason.</description>
        <errorConditionFormula>AND( ISPICKVAL(  StageName  , &quot;Closed Lost&quot;) ,   
           ISPICKVAL(   Opportunity_Lost_Reason__c , &quot;&quot;) 
)</errorConditionFormula>
        <errorDisplayField>Opportunity_Lost_Reason__c</errorDisplayField>
        <errorMessage>All opportunities that have been lost must have a reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Stage_Default</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISNEW(),NOT(ISPICKVAL(StageName,&quot;Position/Prepare&quot;)))</errorConditionFormula>
        <errorMessage>Opportunity should be Position/Prepare when created.</errorMessage>
    </validationRules>
